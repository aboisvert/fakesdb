<?xml version="1.0" encoding="UTF-8"?>
<web-app id="fakesdb" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee">
  <display-name>fakesdb</display-name>
  <servlet>
    <servlet-name>FakeSdbServlet</servlet-name>
    <servlet-class>fakesdb.FakeSdbServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>FakeSdbServlet</servlet-name>
    <url-pattern>/*</url-pattern>
  </servlet-mapping>
</web-app>
